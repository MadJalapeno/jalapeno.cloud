<!DOCTYPE html>
<html lang="en" data-theme="bumblebee">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>{{site.title}}{% if title %} - {{title}} {% endif %}</title>
    <link rel="stylesheet" href="{{ '/static/tw-style.css' | url }}">
<script src="https://cdn.jsdelivr.net/npm/theme-change@2.0.2/index.js"></script>
    <script>
      document.documentElement.setAttribute('data-theme', window.localStorage.getItem('theme'))
    </script>
</head>

<body>
<div class="grid grid-cols-12 gap-1 space-y-4">


<div id="nav" class="col-span-12 container m-auto">  
<!-- nav njk start -->  
{% include "_navigation.njk" %}
<!-- nav njk end -->
</div>


<div class="col-span-12 container m-auto">
<!-- content njk start-->   
{{ content | safe }}
<!-- content njk end -->
</div>

<!-- footer -->
<div class="col-span-12">
{% include "_footer.njk" %}
</div>


</div>
  </body>
</html>

